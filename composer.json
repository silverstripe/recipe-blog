{
    "name": "silverstripe/recipe-blog",
    "type": "silverstripe-recipe",
    "description": "SilverStripe Blog Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^4.5",
        "silverstripe/blog": "3.5.x-dev",
        "silverstripe/widgets": "2.0.x-dev",
        "silverstripe/content-widget": "2.1.x-dev",
        "silverstripe/spamprotection": "3.0.x-dev",
        "silverstripe/akismet": "4.0.x-dev",
        "silverstripe/comments": "3.2.x-dev",
        "silverstripe/comment-notifications": "2.0.x-dev",
        "colymba/gridfield-bulk-editing-tools": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": ["app/*"]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}

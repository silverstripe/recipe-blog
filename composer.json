{
    "name": "silverstripe/recipe-blog",
    "type": "silverstripe-recipe",
    "description": "SilverStripe Blog Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "~2.0.0@stable",
        "silverstripe/recipe-cms": "~5.0.0@rc",
        "silverstripe/blog": "~4.0.0@rc",
        "silverstripe/widgets": "~3.0.0@rc",
        "silverstripe/content-widget": "~3.0.0@rc",
        "silverstripe/spamprotection": "~4.0.0@rc",
        "silverstripe/comments": "~4.0.0@rc",
        "silverstripe/comment-notifications": "~3.0.0@rc",
        "colymba/gridfield-bulk-editing-tools": "~4.0.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "silverstripe/gridfieldqueuedexport": "^3",
        "symbiote/silverstripe-queuedjobs": "^5"
    },
    "conflict": {
        "silverstripe/lumberjack": "<2.1.0"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]

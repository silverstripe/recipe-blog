    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "~4.9.0@beta",
        "silverstripe/blog": "~3.8.0@beta",
        "silverstripe/widgets": "~2.1.1@stable",
        "silverstripe/content-widget": "~2.2.0@stable",
        "silverstripe/spamprotection": "~3.1.0@stable",
        "silverstripe/akismet": "~4.1.0@stable",
        "silverstripe/comments": "~3.6.0@beta",
        "silverstripe/comment-notifications": "~2.1.0@stable",
        "colymba/gridfield-bulk-editing-tools": "^3.0@beta"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7"

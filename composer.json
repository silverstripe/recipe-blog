{
    "name": "silverstripe/recipe-blog",
    "type": "silverstripe-recipe",
    "description": "SilverStripe Blog Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "~4.10.0@beta",
        "silverstripe/blog": "~3.9.0@beta",
        "silverstripe/widgets": "~2.2.0@stable",
        "silverstripe/content-widget": "~2.3.0@stable",
        "silverstripe/spamprotection": "~3.2.0@beta",
        "silverstripe/akismet": "~4.2.0@beta",
        "silverstripe/comments": "~3.7.2@stable",
        "silverstripe/comment-notifications": "~2.2.0@beta",
        "colymba/gridfield-bulk-editing-tools": "^3.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}